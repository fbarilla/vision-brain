                    "required": true
                },
                "-r": {
                    "name": "Requirements file",
                    "description": "Select a text file to use as a list of pip requirements (e.g. requirements.txt)",
                    "type": "FILE",
                    "positional": false,
                    "required": false,
                    "filter": "*.txt"
                }
            }
        }
    },
    "image": {
        "type": "image/png",
        "data": ""
